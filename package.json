{
  "name": "occup.ie",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "if test \"$NODE_ENV\" = \"production\"; then npm install pm2 -g; fi;",
    "start": "if test \"$NODE_ENV\" = \"production\"; then pm2-runtime ./bin/www -i max; else node ./bin/www; fi;",
    "test": "mocha 'test/**/*.js' --exit"
  },
  "nyc": {
    "include": [
      "models/**/*.js",
      "routes/**/*.js"
    ],
    "exclude": [
      "config/ops/**/*.js",
      "routes/v1/endpoints/index.js",
      "models/use_cases/common/**/*.js"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.231.1",
    "aws-sdk-apis": "^3.1.10",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "dynamodb": "^1.1.1",
    "expect": "^23.0.0",
    "express": "^4.16.3",
    "express-handlebars": "*",
    "express-serve-static-core": "^0.1.1",
    "fcm-push": "^1.1.3",
    "fetch": "^1.1.0",
    "jade": "~1.11.0",
    "joi": "^13.2.0",
    "mocha": "^5.1.1",
    "mongodb": "^3.0.7",
    "monk": "^6.0.5",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
