    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.5",
    "npm": "^5.8.0",
    "nyc": "^11.7.1",
    "s3rver": "^2.2.4",
    "serve-favicon": "~2.4.5",
    "serve-static": "^1.13.2",
